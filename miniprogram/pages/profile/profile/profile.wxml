<view class="top radius" style="padding:20rpx">
	<image class="top-bc" src="{{bgUrl._url}}"></image>
</view>

<view class="cu-list menu  margin-bottom-xl shadow-lg radius" style='margin:20rpx;border-radius: 20rpx'>
	<view class="cu-item arrow">
		<navigator class="content" url="/pages/profile/about/about" hover-class="none">
			<image src="https://7072-production-ue9j7-1301900827.tcb.qcloud.la/huibiao/huibiao-1.png?sign=f01a9bc1c2d0ac195991adc9161fdb52&t=1587369702" class="png" mode="aspectFit"></image>
			<text class="text-grey">关于我们</text>
		</navigator>
	</view>
	<view class="cu-item arrow">
		<button class="cu-btn content" open-type="feedback" hover-class="none">
			<text class="cuIcon-writefill text-green"></text>
			<text class="text-grey">意见反馈</text>
		</button>
	</view>
	<view class="cu-item arrow">
		<view class="content" bindtap="goWNIdentify" hover-class="none">
			<text class="cuIcon-creativefill text-orange"></text>
			<text class="text-grey">舞美认证</text>
		</view>
	</view>
	<view class="cu-item arrow">
		<view class="content" bindtap="getZzStNumberInfo" hover-class="none">
			<text class="cuIcon-light text-orange"></text>
			<text class="text-grey">组织/社团认证</text>
		</view>
	</view>

	<view class="cu-item arrow">
		<button class="cu-btn content" open-type="share" hover-class="none">
			<text class="cuIcon-share text-green"></text>
			<text class="text-grey">分享小程序</text>
		</button>
	</view>

</view>